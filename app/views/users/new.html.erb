<h1>ユーザー登録</h1>
<%= form_with(model: @user, local: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.submit "新規ユーザー登録", class: "btn btn-primary" %>
<% end %>
